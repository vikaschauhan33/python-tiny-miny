#Fibonacci sequence

def fibonacci(num):
    print "---{}--{}--------".format(__name__, num)
    a = 0
    b = 1

    for i in xrange (0,num):
        print a;
        a,b=b,a+b
    #print "----> {}".format(b)


#Factorial

def factorial(num):
    print "---{}--{}--------".format(__name__, num)
    a = 1
    for i in xrange(1,num+1):
        a = a*i

    print a

if __name__ == '__main__' :
    factorial(6)
    fibonacci(5)